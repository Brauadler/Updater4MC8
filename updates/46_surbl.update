#!/bin/bash
export PATH="/usr/mailcleaner/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"

if [ -e /usr/mailcleaner/etc/rbls/SURBL.cf ]; then
    rm /usr/mailcleaner/etc/rbls/SURBL.cf
fi

if [ -e /var/mailcleaner/tmp/fetch_files/usr/mailcleaner/etc/rbls/SURBL.cf ]; then
    rm /var/mailcleaner/tmp/fetch_files/usr/mailcleaner/etc/rbls/SURBL.cf
fi
